@model IEnumerable<DoAnChuyenNganhBookingHotel.Models.DB.TrangThaiPhong>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<p>
    <div style="padding-top:3px">
        @Html.ActionLink("Tạo Mới", "Create", null, new { @class = "btn btn-outline-secondary" })
    </div>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.trangthaiphong1)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.trangthaiphong1)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.trangthaiphong_id }, new { @class = "btn btn-outline-success" })
        @Html.ActionLink("Details", "Details", new { id = item.trangthaiphong_id }, new { @class = "btn btn-outline-warning" })
        @Html.ActionLink("Delete", "Delete", new { id = item.trangthaiphong_id }, new { @class = "btn btn-outline-danger" })
    </td>
</tr>}

</table>
